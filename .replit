modules = ["nodejs-20", "web", "postgresql-16"]
[agent]
expertMode = true
integrations = ["javascript_openai:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["unzip"]

[deployment]
deploymentTarget = "static"
build = ["npm", "run", "build"]
